{
  "name": "hyperspell-mcp-server",
  "version": "0.1.0",
  "description": "Hyperspell MCP Server",
  "private": true,
  "type": "module",
  "bin": {
    "hyperspell-mcp-server": "./build/index.js"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsc && node -e \"require('fs').chmodSync('build/index.js', '755')\"",
